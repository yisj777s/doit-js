//12.HTTP통신과 JSON
//12-1.HTTP 통신
//12-2.JSON
//12-2-1.JSON이란
//JSON의 형식
undefined
// {
//     "이름" : 값,
//         ...
// }
undefined
// 객체로 표현할 때
{
    name : "도레미",
    major : " 컴퓨터 공학",
    grade : 2
}
// JSON 으로 표현할 때
{
    "name" : "도레미",
    "major" : "컴퓨터 공학",
    "grade" : 2
}
// 프로그램에서 JSON 인식 - 하나의 문자열(JSON문자열)
// '{ "name" : "도레미", "major" : "컴퓨터 공학", "grade" : 2}'
//JSON의 이름
undefined
// 맞게 사용한 예
{ "name" : "도레미"}
// 잘못 사용한 예
undefined
{ 'name' : "도레미"}
// 공백, 하이픈, 언더바 사용가능
{ "full name" : "도레미"}
{ "full-name" : " 도레미"}
{ "full_name" : "도레미"}
// JSON의 값
// 숫자, 문자열, 논리값, null, 배열, 객체
// 1. 숫자열 : 정수,실수o, 8진수,16진수x
48, -72, 25.8, -8.4, 2.3e4
// 2. 문자열 : 작은따옴표x, 큰따옴표o
"자바스크립트", "HTML5"
'HTML5'
// 3. 논리값과 null
// 4. JSON 문자열과 배열
{ 
    "name" : "도레미",
    "major" : "컴퓨터 공학",
    "grade" : 2,
    "course" : ["웹 기초", "자바스크립트", "인공지능"]
}
{ 
    "name" : "도레미",
    "major" : "컴퓨터 공학",
    "grade" : 2,
    "course" : {
        "title" : "웹 기초",
        "timePerWeek" : 3
    }
}
