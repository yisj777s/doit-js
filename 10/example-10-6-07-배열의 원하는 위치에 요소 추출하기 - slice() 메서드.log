// 배열의 원하는 위치에 있는 요소 추출 - slice() 메서드
let colors = ["red", "green", "blue", "white", "black"]
undefined
// 인수지정하지않기 = 전체 요소 추출
colors.slice()
(5) ['red', 'green', 'blue', 'white', 'black']
// 시작 위치 지정 = 시작위치부터 끝까지 추출
colors.slice(2)
(3) ['blue', 'white', 'black']
colors
(5) ['red', 'green', 'blue', 'white', 'black']
// 끝 위치 지정 = 끝 위치 직전까지 추출
let colors2 = colors.slice(1, 4)
undefined
colors2
(3) ['green', 'blue', 'white']
colors
(5) ['red', 'green', 'blue', 'white', 'black']
colors.slice(2, 4)
(2) ['blue', 'white']
